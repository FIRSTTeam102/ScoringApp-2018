extends layout

block content
	main(class="w3-mobile w3-center" style="font-size:1.2em")
		h3(class="gear-text-white")=title
		div(class="w3-container w3-padding-16")
			h4(class="gear-text-white") Member Management
			a(href="/admin/scoutingpairs") 
				div(class="gear-btn gear-orange gear-hover-white w3-btn w3-section gear-margin-sides")
					span Scouting pairs
			a(href="/admin/teammembers") 
				div(class="gear-btn gear-orange gear-hover-white w3-btn w3-section gear-margin-sides")
					span Manage Members
			a(href="/admin/teammembers/present") 
				div(class="gear-btn gear-orange gear-hover-white w3-btn w3-section gear-margin-sides")
					span Set who is present at current event
