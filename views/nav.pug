if user
	nav(class="w3-bar gear-opacity w3-container gear-text-white" )
		div(href="#" class="w3-dropdown-hover w3-dropdown-mobile gear-clear gear-text-white w3-text-black-hover")
			if user.name != "[Dev]"
				div(class="w3-button")=user.name
				div(class="w3-dropdown-content w3-bar-block")
					a(href="/logout" class="w3-bar-item w3-button") Log out
			else
				div(class="w3-bar-item")=user.name
		a(href="/scoring" class="w3-bar-item w3-button gear-clear") Scoring App
		a(href="/scouting" class="w3-bar-item w3-button gear-clear") Scouting Survey
		if user.subteam == "support" || user.subteam == "exec"
			a(href="/admin" class="w3-bar-item w3-button gear-clear") Admin Index
			a(href="/tests" class="w3-bar-item w3-button gear-clear") Test pages
	br
header(class="w3-center w3-container w3-content w3-padding-8")
	a(href="/") 
		div(style="max-width:400px" class="w3-content")
			img(src="/images/powerup-header-glow-400px.png" class="w3-image")
	h4(class="gear-text-white" style="margin:0px;padding:0px;")=tournament
	br 