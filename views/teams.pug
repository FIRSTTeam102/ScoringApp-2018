extends layout

block content
	form#updateForEvent(name="updateForEvent" method="post" action="/admin/data/teams")
		input#seteventid(type="text", name="eventId")
		button#btnSubmit(type="submit") Update DB for specified event ID
	if locals.teams
		table(class="w3-table")
			tr
				th Number
				th City
				th Nickname
				th Name
			each team in teams
				tr
					td= team.team_number
					td= team.city
					td= team.nickname
					td= team.name
					td
						button #{team.key}
	